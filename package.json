{
  "name": "cy_aps",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "node ./.husky/prepare.js",
    "pre-commit": "NODE_ENV=production vue-tsc --noEmit && lint-staged",
    "commit-msg": "commitlint -e",
    "commit": "git cz",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "genConfig": "node ./deploy/genNginxConf.js",
    "preview": "vite preview",
    "build:dev": "cross-env ENV_NAME=dev npm run genConfig && vue-tsc --noEmit && vite build --mode dev",
    "build:test": "cross-env ENV_NAME=test npm run genConfig && vue-tsc --noEmit && vite build --mode test",
    "build:prod": "cross-env ENV_NAME=prod npm run genConfig && vue-tsc --noEmit && vite build --mode prod"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.33.1",
    "@vitejs/plugin-vue": "^3.0.1",
    "axios": "^0.24.0",
    "sass": "^1.54.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2",
    "vue-tsc": "^0.39.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "prettier --write",
      "eslint --ext .js,.ts,.vue  src/"
    ]
  },
  "devDependencies": {
    "@babel/types": "^7.18.9",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "prettier": "^2.7.1",
    "vue-eslint-parser": "^8.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
