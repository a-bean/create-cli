{
  "name": "cy_aps",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "node ./.husky/prepare.js",
    "pre-commit": "NODE_ENV=production vue-tsc --noEmit && lint-staged",
    "commit-msg": "commitlint -e",
    "commit": "git cz",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "genConfig": "node ./deploy/genNginxConf.js",
    "preview": "vite preview",
    "build:dev": "npm run genConfig && vue-tsc --noEmit && vite build --mode dev",
    "build:test": "npm run genConfig && vue-tsc --noEmit && vite build --mode test",
    "build:demo": "npm run genConfig && vue-tsc --noEmit && vite build --mode demo",
    "build:prd": "npm run genConfig && vue-tsc --noEmit && vite build --mode prd"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.26.0",
    "@vitejs/plugin-vue": "^2.3.2",
    "axios": "^0.24.0",
    "sass": "^1.51.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15",
    "vue-tsc": "^0.34.11"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "prettier --write",
      "eslint --ext .js,.ts,.vue  src/"
    ]
  },
  "devDependencies": {
    "@babel/types": "^7.17.10",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "vue-eslint-parser": "^8.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
